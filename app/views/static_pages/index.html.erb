<%= form_with url: root_path, method: :get do |f|%>
    <%= f.label :id%>
    <%= f.text_field :id%>
    <%= f.submit "Submit"%>
<% end %>

<% @photos.each do |p|%>
    <%= image_tag(Flickr.url_n(p)) %><br>
    <%= form_with url: static_page_path(p.id), method: :get do |f|%>
        <%= f.hidden_field :photo_id, value: p.id %>
        <%= f.submit "Photo info"%>
    <% end %>
<% end %>